{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Damar Albaribin <damaralbaribin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NO_PEER_DEPENDENCY_CHECK='true' PEER_DEPENDENCY_CHECK='false' ts-node --transpile-only src/main.ts",
    "seed": "prisma migrate reset -f && NO_PEER_DEPENDENCY_CHECK='true' PEER_DEPENDENCY_CHECK='false' ts-node-dev --transpile-only prisma/seed.ts",
    "queue": "NO_PEER_DEPENDENCY_CHECK='true' PEER_DEPENDENCY_CHECK='false' ts-node-dev --transpile-only queue/fileprocessor.ts",
    "dev": "prisma generate && NO_PEER_DEPENDENCY_CHECK='true' PEER_DEPENDENCY_CHECK='false' ts-node-dev --transpile-only src/main.ts",
    "pm2:start:gql": "pm2 start npm --name 'graphql' -- run 'start'",
    "pm2:start:queue": "pm2 start npm --name 'queue' -- run 'queue'"
  },
  "dependencies": {
    "@prisma/client": "^3.7.0",
    "@sitespeed.io/throttle": "^3.1.0",
    "@types/graphql-iso-date": "^3.4.0",
    "@vercel/ncc": "^0.33.1",
    "apollo-server": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "bcrypt": "^5.0.1",
    "bee-queue": "^1.4.0",
    "bull": "^4.2.0",
    "bullmq": "^1.59.4",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "graphql": "^16.2.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-scalars": "^1.14.1",
    "graphql-upload": "^13.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mediasoup": "^3.9.2",
    "nexus": "next",
    "nexus-prisma": "0.35.0-next.4",
    "pino": "^7.5.1",
    "pino-pretty": "^7.3.0",
    "prisma": "^3.7.0",
    "socket.io": "^4.4.0",
    "socket.io-server": "^1.0.0-b",
    "uuid": "^8.3.2",
    "uuidjs": "^4.2.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/graphql-upload": "^8.0.7",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.178",
    "@types/pino": "^7.0.5",
    "@types/uuid": "^8.3.3",
    "@types/uuidjs": "^3.6.0",
    "@webpack-cli/generators": "^2.4.1",
    "pm2": "^5.1.2",
    "ts-loader": "^9.2.6",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "description": "My webpack project"
}
